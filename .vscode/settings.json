{
  "files.exclude": {
    "**/*.pyc": {"when": "$(basename).py"},
    "**/__pycache__": true,
    "**/.pytest_cache": true
  },
  "python.linting.enabled": true,
  "python.linging.pylintEnable":true,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "python.linting.pylintArgs": ["--rcfile=${workspaceFolder}/.pylintrc"]
}